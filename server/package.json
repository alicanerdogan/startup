{
  "name": "nails",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.6",
    "@types/node-fetch": "^2.5.7",
    "@types/supertest": "^2.0.9",
    "@types/uuid": "^8.0.0",
    "@types/validator": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "knex-db-manager": "^0.6.1",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.1",
    "rate-limiter-flexible": "^2.1.6",
    "uuid": "^8.1.0",
    "validator": "^13.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "start": "nodemon --exec ts-node src/index.ts",
    "format": "prettier --write \"src/**/*.{ts*,js*}\"",
    "lint": "eslint \"src/**/*.{ts*,js*}\" -c .eslintrc.js",
    "migration:new": "yarn ts-node ./node_modules/knex/bin/cli.js migrate:make --knexfile ./src/dbConfig.js -x ts",
    "migration:list": "ts-node ./node_modules/knex/bin/cli.js migrate:list --knexfile ./src/dbConfig.js",
    "migrate": "ts-node ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./src/dbConfig.js",
    "rollback": "ts-node ./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./src/dbConfig.js",
    "test": "jest"
  }
}
